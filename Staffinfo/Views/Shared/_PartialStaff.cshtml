@model Staffinfo.Models.StaffViewModel
@{
    Layout = null;
}
@foreach (var item in Model.StaffList)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.FullName</td>
        <td>@item.Address</td>
        <td>@item.Gender</td>
        <td>
            <img src="@item.FilePath" height="100" width="100" />
        </td>
    </tr>
}